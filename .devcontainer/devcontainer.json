{
  "name": "ansible-dev",
  "dockerFile": "Dockerfile",
  "context": "..",
  "remoteUser": "ansible",
  "workspaceFolder": "/workspace",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=delegated",
  "runArgs": ["-h","ansible-dev", "--name", "ansible-dev"],
  "customizations": {
    "vscode": {
      "extensions": [
        "hashicorp.terraform",
        "hashicorp.hcl",
        "redhat.ansible"
      ]
    }
  }
}